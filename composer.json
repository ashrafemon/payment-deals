{
    "name": "leafwrap/payment-deals",
    "description": "A Laravel-based online payment gateway solution featuring popular payment gateways like PayPal, Stripe, Razorpay, bKash, and more. Seamlessly integrate secure and diverse payment options into your web applications with ease.",
    "type": "library",
    "version": "2.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Leafwrap\\PaymentDeals\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ashraf Emon",
            "email": "ashraf.emon143@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leafwrap\\PaymentDeals\\PaymentDealServiceProvider"
            ],
            "aliases": {
                "PaymentDeal": "Leafwrap\\PaymentDeals\\Facades\\PaymentDeal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
